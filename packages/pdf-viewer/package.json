{
  "name": "@cannyminds/pdf-viewer",
  "version": "0.3.0",
  "description": "A React Headless PDF viewer component built with @embedpdf",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./workers/*": "./dist/workers/*"
  },
  "type": "module",
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "pdf",
    "viewer",
    "embedpdf",
    "component"
  ],
  "author": "CannyMinds Dev Team",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "demo.png"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/CannyMinds/PDF-Viewer-Lib.git",
    "directory": "packages/pdf-viewer"
  },
  "packageManager": "pnpm@10.10.0",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@embedpdf/core": "^1.4.1",
    "@embedpdf/engines": "^1.4.1",
    "@embedpdf/models": "^1.4.1",
    "@embedpdf/pdfium": "^1.4.1",
    "@embedpdf/plugin-annotation": "^1.4.1",
    "@embedpdf/plugin-export": "^1.4.1",
    "@embedpdf/plugin-history": "^1.4.1",
    "@embedpdf/plugin-interaction-manager": "^1.4.1",
    "@embedpdf/plugin-loader": "^1.4.1",
    "@embedpdf/plugin-render": "^1.4.1",
    "@embedpdf/plugin-scroll": "^1.4.1",
    "@embedpdf/plugin-search": "^1.4.1",
    "@embedpdf/plugin-selection": "^1.4.1",
    "@embedpdf/plugin-viewport": "^1.4.1",
    "@embedpdf/plugin-zoom": "^1.4.1"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}